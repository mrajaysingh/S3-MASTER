{
    "name": "mrajaysingh/s3-master",
    "description": "Complete AWS S3 integration for WordPress with file management, automatic media backup, and GitHub-based plugin updates.",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin", "s3", "aws", "backup", "storage", "media", "file-manager", "cloud"],
    "homepage": "https://github.com/mrajaysingh/S3-MASTER",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Ajay Singh",
            "email": "mrajaysingh@example.com",
            "homepage": "https://github.com/mrajaysingh",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mrajaysingh/S3-MASTER/issues",
        "source": "https://github.com/mrajaysingh/S3-MASTER"
    },
    "require": {
        "php": ">=7.4",
        "aws/aws-sdk-php": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "wp-coding-standards/wpcs": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "S3Master\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "S3Master\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress --extensions=php --ignore=vendor/ .",
        "phpcbf": "phpcbf --standard=WordPress --extensions=php --ignore=vendor/ .",
        "post-install-cmd": [
            "@composer dump-autoload"
        ],
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
